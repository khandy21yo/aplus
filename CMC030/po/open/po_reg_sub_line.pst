	!======================================================================
	! PO_REG_SUB_LINE file (create, modify, allow read)
	!======================================================================

	CALL ASSG_CHANNEL(PO_REG_SUB_LINE.CH%, STAT%)
	CALL READ_DEVICE('PO_REG_SUB_LINE',PO_REG_SUB_LINE.DEV$, STAT%)
	CALL READ_PROTECTION('PO_REG_SUB_LINE',PO_REG_SUB_LINE.PRO$,STAT%)
	CALL READ_CURPROTECTION(OLD_PROT$,STAT%)
	CALL WRIT_CURPROTECTION(PO_REG_SUB_LINE.PRO$, STAT%)

	PO_REG_SUB_LINE.NAME$ = PO_REG_SUB_LINE.DEV$+"PO_REG_SUB_LINE.LED"

	OPEN PO_REG_SUB_LINE.NAME$ AS FILE PO_REG_SUB_LINE.CH%, &
		ORGANIZATION INDEXED FIXED, &
		MAP PO_REG_SUB_LINE, &
		BUFFER 32%, &
		PRIMARY KEY &
		( &
			PO_REG_SUB_LINE::PO, &
			PO_REG_SUB_LINE::PO_LINE, &
			PO_REG_SUB_LINE::PO_ACTION &
		)	DUPLICATES , &
		ALTERNATE KEY &
		( &
			PO_REG_SUB_LINE::BATCH, &
			PO_REG_SUB_LINE::PO, &
			PO_REG_SUB_LINE::PO_LINE &
		)	DUPLICATES CHANGES, &
		ACCESS MODIFY, ALLOW READ

	CALL WRIT_CURPROTECTION(OLD_PROT$, STAT%)

